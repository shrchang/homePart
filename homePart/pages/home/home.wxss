.head{
  width: 100%;
  height: 8rem;
  top: 0px;
}

.home-head{
  width: 100%;
  height: 8rem;
}

.middle{
  margin-top: 20rpx;
  overflow: hidden;
}

.middle-title{
  height: 2rem;
  text-align: center;
}

.middle-title .middle-text{
  border: solid 1rpx #e6e6e6;
  height: 3rem;
  display: block;
  border-radius: 10rpx;
  margin: 30rpx 10rpx;
  box-shadow: 0 0 10rpx #ccc;
}

.middle-title .middle-text image{
  width: 60rpx;
  height: 60rpx;
  float: left;
  line-height: 3rem;
  margin: 0.5rem 20rpx;
}

.middle-title .middle-text div{
  width: 60%;
  float: left;
  margin-top: 10rpx;
}

.middle-text span{
  display: block;
  text-align: left;
  font-size: 20rpx;
  color: #696969;
  margin-top: 4rpx;
  margin-left: 20rpx;
}

.middle-text span.span-title{
  color: #000;
  font-size: 30rpx;
}

.middle-title .middle-text button{
  width:130rpx;
  height:60rpx;
  background-repeat:no-repeat;
  background-size:100% 100%;
  float:left;
  font-size:25rpx;
  margin-top:20rpx;
  color: #fff;
  border-radius: 25rpx;
  background-color:#20B2AA;
}

.middle .btn-group{
  float: left;
  font-size: 20rpx;
  border:0rem;
  margin: 0 6%;
}

.middle .btn-group div{
  display: block;
  width: 90rpx;
  height: 90rpx; 
  border-radius: 50%;
  border: solid 3rpx #e6e6e6;
  text-align: center;
  cursor: pointer;
}

.middle .btn-group image{
  margin-top: 15rpx;
  width: 60rpx;
  height: 60rpx;
}

.middle .btn-group span{
  margin-left: 10rpx;
  margin-top: 20rpx;
}

.md-header{
  width: 100%;
  margin-top: 30rpx;
  display: block;
  text-align: center;
}

.md-header hr{
  height: 1rpx;
  width: 120rpx;
  border: none;
  border-top: 1rpx solid #ddd;
  display: inline-block;
  margin-bottom: 15rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.scroll-view{
  height: 300rpx;
  width: auto;
  overflow: hidden;
  margin-top: 20rpx;
}

.scroll-view .scroll-room{
  height: 300rpx;
  width: 30%;
  display: inline-block;
  margin: 0 20rpx;
}

.scroll-room image{
  width: 100%;
}

.scroll_box{
  width: 100%;
  height: 300rpx;
  overflow: hidden;
  padding: 20rpx;
  background: #fff;
  white-space: nowrap;
}

.backgroundview {
  height: 300rpx;
  width: 100vw;
  position: absolute;
}

.qswiper-block {
  height: 280rpx;
  width: 100vw;
  margin-top: 25rpx;
}

.moveImg{
  margin-top: 35rpx;
  height: 280rpx;
}

.qswiper-item {
  margin-top: 60rpx;
  height: 160rpx;
  width: 120rpx;
  overflow: unset;
}

.qswiper-item {
  height: 160rpx;
  overflow: unset;
}

.qslide-image {
  height: 280rpx;
  width: 130rpx;
  border-radius: 9rpx;
  box-shadow: 0rpx 0rpx 10rpx rgba(0, 0, 0, 0.2);
  margin: 0rpx 3rpx;
  z-index: 1;
}

.qswiper-item .active {
  transform: scale(1.1);
  transition: all 0.2s ease-in 0s;
  z-index: 20;
}

.blur {
  position: absolute;
  padding: 1rpx;
  box-sizing: border-box;
  z-index: 0;
  background-repeat: no-repeat;
  background-size:100vw 300rpx;
}

.blur:after {
  content: "";
  width: 100vw;
  height: 100vw;
  position: absolute;
  left: 0;
  top: 0;
  background: inherit;
  filter: blur(3rpx);
  z-index: 2;
}

.qswiper-item .active {
  height: 160rpx;
  width: 120rpx;
  transform: scale(1.3);
  transition: all 0.2s ease-in 0s;
  z-index: 20;
  margin-bottom: 48rpx;
  
  background-image: url("data:image/png;base64,");
  border: 6rpx solid #ffff;
}

.ccmc{
  height: 100rpx;
  display: flex;
  text-align: center;
  flex-direction: column;
  font-family: 'PingFang SC Medium';
  font-size: 36rpx;
  margin-top: 20rpx;
}