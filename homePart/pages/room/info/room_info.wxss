  z-index: 20;
  margin-bottom: 48rpx;
  background-image: url("data:image/png;base64,");
  border: 6rpx solid #ffff;
}

.blur {
  position: absolute;
  padding: 1rpx;
  box-sizing: border-box;
  z-index: 0;
  background-repeat: no-repeat;
  background-size: 100vw 270rpx;
}

.blur:after {
  content: "";
  width: 100vw;
  height: 100vw;
  position: absolute;
  left: 0;
  top: 0;
  background: inherit;
  filter: blur(3rpx);
  z-index: 2;
}

.drag {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 160rpx;
  height: 180rpx;
  text-align: center;
  z-index: 11;
}

