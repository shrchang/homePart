/* pages/room/usercomment/comment.wxss */
.font-class{
  color: red
}

.card{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-content: center;
  margin-top: 30rpx;
}

.card-item{
  width: 620rpx;
  height: 350rpx;
  border: 1px solid rgba(109, 106, 106, 0.021);
  box-shadow: 1rpx 1rpx 50rpx 1rpx rgba(196, 192, 192, 0.349);
}

.head-image{
  margin-left:2%;
  margin-top: -42%;
  position: relative;
  width: 80rpx;
  height: 80rpx;
  border: 1px solid rgba(109, 106, 106, 0.021);
  border-radius: 100rpx;
  box-shadow: 1rpx 1rpx 50rpx 1rpx rgba(196, 192, 192, 0.349);
  background-image: url("data:image/jpg;base64,");
  background-size: 80rpx 80rpx;
}
.user-score{
  display: flex;
  justify-content: row;
}
.user-score image{ 
 height: 20rpx; 
 width: 20rpx; 
 display: inline-block 
} 

.card-item-top{
  display: flex;
  justify-content: space-between;
  width: 75%;
  margin-left: 17%;
  margin-top: 7%;
}

.user-name{
  font-size: 22rpx;
  font-family: PingFangSC-Regular;
  color: rgb(102, 102, 102)
}

.shape-image{
  width: 30rpx;
  height: 30rpx;
}
.card-item-top-right{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.shape-count{
  margin-top: 9rpx;
  margin-left: 20rpx;
  font-size: 14rpx;
  color: rgb(94, 177, 152)
}

.card-item-center{
  margin-left: 17%;
  margin-top: 10rpx;
}

.card-item-bottom{
  margin-top: 30rpx;
  display: flex;
  flex-direction: column;
}

.card-item-bottom{
  margin-left: 17%;
}

.comment-text{
  font-size: 22rpx;
  font-family: PingFangSC-Regular;
  color: rgb(102, 102, 102);
}

.comment-image{
  margin-top: 20rpx;
  width: 100vw;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.comment-image image{
  width: 160rpx;
}